<resources>
    <!-- Simple Calculator theme. -->
    <style name="SimpleCalcTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Complete Calculator theme. -->
    <style name="CompleteCalcTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorSecondary</item>
        <item name="colorPrimaryDark">@color/colorSecondaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
</resources>